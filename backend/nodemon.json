{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "cache/**/*"
  ],
  "verbose": false,
  "execMap": {
    "js": "node"
  },
  "events": {
    "restart": "npm run kill-port"
  },
  "watch": [
    "**/*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}